% cumple la condicion
array[1..N] of var bool: cumple;

% pertenece o es vecino
constraint forall(i in 1..N)(
  cumple[i] \/ exists(j in 1..N where adyacencia[i,j] == 1)(cumple[j])
);

% resuelve
solve minimize sum(cumple);

% output
output ["Mínimo número de nodos en el conjunto: ", show(sum(cumple))];


